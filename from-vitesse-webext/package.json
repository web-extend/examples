{
  "name": "vitesse-webext",
  "displayName": "Vitesse WebExt",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "Migrate to WebExtend",
  "scripts": {
    "dev": "web-extend rsbuild:dev --open",
    "build": "web-extend rsbuild:build",
    "preview": "web-extend preview",
    "zip": "web-extend zip",
    "dev:firefox": "web-extend rsbuild:dev --open --target firefox-mv2",
    "build:firefox": "web-extend rsbuild:build --target firefox-mv2",
    "zip:firefox": "web-extend zip --target firefox-mv2",
    "preview:firefox": "web-extend preview --target firefox-mv2",
    "lint": "eslint --cache .",
    "test": "vitest test",
    "test:e2e": "playwright test",
    "postinstall": "simple-git-hooks",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.0",
    "@iconify/json": "^2.2.239",
    "@playwright/test": "^1.46.1",
    "@rsbuild/core": "^1.2.11",
    "@rsbuild/plugin-vue": "^1.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.5.0",
    "@types/webextension-polyfill": "^0.12.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@unocss/postcss": "^66.0.0",
    "@unocss/reset": "^66.0.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/compiler-sfc": "^3.4.38",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^11.0.1",
    "@web-extend/rsbuild-plugin": "0.0.1-beta.5",
    "eslint": "^9.9.0",
    "fs-extra": "^11.2.0",
    "jsdom": "^24.1.1",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.2.9",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.4",
    "unocss": "^66.0.0",
    "unplugin-auto-import": "^19.1.0",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.2",
    "vitest": "^2.0.5",
    "vue": "^3.5.13",
    "web-ext": "^8.2.0",
    "web-extend": "0.0.1-beta.5",
    "webext-bridge": "^6.0.1",
    "webextension-polyfill": "^0.12.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "./*": "eslint --fix"
  }
}
